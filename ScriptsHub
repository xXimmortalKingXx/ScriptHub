    print("Clicked")
    loadstring(game:HttpGet("https://HugeGames.io/script"))()
end)
-- MM2
local MM2 = Window:NewTab("Murderer Mystery 2")

local MM2Section = MM2:NewSection("Murderer Mystery 2")
MM2Section:NewButton("Murderer Mystery 2 script", "Eclipse MM2 script.", function()
    print("Clicked")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Ethanoj1/EclipseMM2/master/Script", true))()
end)
-- Horrific H0using
local HOHO = Window:NewTab("Horrific H0using")

local HOHOSection = HOHO:NewSection("Horrific H0using")
HOHOSection:NewButton("Horrific H0using INF money script", "click to get tons of coins!", function()
    print("Clicked")
    while true do
        wait(0)
        local Target = game:GetService("ReplicatedStorage").LoginRewards;
        Target:FireServer(100000000000);
        end
end)
